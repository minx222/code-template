    "react-redux": "^8.0.5",
    "react-router-dom": "6",
    "redux": "^4.2.1"
  },
  "devDependencies": {
    "husky": "^9.0.10",
    "cz-git": "^1.9.0",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@biomejs/biome": "^1.8.3",
    "@rsbuild/core": "1.0.0-alpha.8",
    "@rsbuild/plugin-react": "1.0.0-alpha.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "postcss-loader": "^8.1.1",
    "commitizen": "^4.3.0",
    "sass": "^1.77.8",
